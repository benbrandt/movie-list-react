{
  "extends": [
    "standard",
    "standard-react",
    "plugin:flowtype/recommended",
    "prettier",
    "prettier/flowtype",
    "prettier/react"
  ],
  "plugins": [
    "react",
    "flowtype",
    "prettier"
  ],
  "env": {
    "browser": true,
    "jest": true,
  },
  "rules": {
    "react/no-unused-prop-types": "off",
    "prettier/prettier": "error"
  }
}
